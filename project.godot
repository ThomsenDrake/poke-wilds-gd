; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PokeWilds-GD"
config/description="A Godot port of PokeWilds - An open-world Pokemon survival game"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/autoloads/game_manager.gd"
GameLogger="*res://scripts/autoloads/game_logger.gd"
InputManager="*res://scripts/autoloads/input_manager.gd"
AudioManager="*res://scripts/autoloads/audio_manager.gd"
SaveManager="*res://scripts/autoloads/save_manager.gd"
TypeChart="*res://scripts/autoloads/type_chart.gd"
SpeciesDatabase="*res://scripts/autoloads/species_database.gd"
MoveDatabase="*res://scripts/autoloads/move_database.gd"
ItemDatabase="*res://scripts/autoloads/item_database.gd"
BattleManager="*res://scripts/autoloads/battle_manager.gd"
FieldMoveManager="*res://scripts/autoloads/field_move_manager.gd"
StructureDatabase="*res://scripts/autoloads/structure_database.gd"
BuildManager="*res://scripts/autoloads/build_manager.gd"
HabitatManager="*res://scripts/autoloads/habitat_manager.gd"
BreedingManager="*res://scripts/autoloads/breeding_manager.gd"
RanchManager="*res://scripts/autoloads/ranch_manager.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=432
window/size/window_width_override=960
window/size/window_height_override=864
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_mcp/plugin.cfg")

